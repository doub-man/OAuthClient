<% if @current_user %>
  <div>
    <label>当前登陆用户是: </label>
    <%= @current_user.email %>
    <span style="padding-left: 200px">
      <%= link_to '退出', session_path(1), method: 'delete' %>
    </span>
  </div>
<% end %>
<hr>
<div class="normal">
  <h4>传统登陆方式</h4>
  <%= form_for :session, url: sessions_path do |f| %>
    <label>邮箱</label>
    <%= f.text_field :email %>
    <%= f.submit '登陆' %>
  <% end %>
</div>
<hr>
<div class="oauth">
  <h4>OAuth2登陆方式</h4>
  <%= link_to 'todo账号登录', '/auth/todo' %>
</div>
